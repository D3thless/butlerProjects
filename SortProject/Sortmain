import java.io.*;
import java.util.*;

public class Sorts extends SortMethods
{
	private static int[] Reset()
	{
		int [] storedValues;
		int [] resetArray=new int [0];
		storedValues=resetArray;
		return storedValues;
	}
	public static void main(String[] args) 
	{
		int enteredNum=0;
		System.out.println("Please enter the number of values in your array: ");
		Scanner cin=new Scanner(System.in);
		int [] storedValues= new int [0];
		for (int i=0;i<enteredNum;i++) 
		{
			int arrayNum=(int) (1 + (enteredNum * Math.random()));
			storedValues[i]=arrayNum;
		}
		if (enteredNum<=100)
		{
			System.out.println(storedValues);
		}
	}
}
